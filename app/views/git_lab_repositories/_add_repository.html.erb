<%= form_tag git_lab_repository_create_path(project), class: "add-repository-form" do %>
  <%= text_field_tag :title, '', placeholder: 'Title', required: true %>
  <%= text_field_tag :repository_url, '', placeholder: 'https://gitlab.com/user/repository.git', required: true %>
  <a class="flaticon-accept approve_btn"></a>
  <a class="flaticon-prohibition close_btn"></a>
<% end %>
